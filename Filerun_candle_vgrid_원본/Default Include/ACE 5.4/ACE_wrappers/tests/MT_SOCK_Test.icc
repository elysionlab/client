    incl(searchPath, ".."),
    link(linkWithMultiThreadLib,yes),
    link(debug)
    {
	target type (exe) "MT_SOCK_Test"
	{
	   source type (cpp) "MT_SOCK_Test.cpp", "Main.cpp"
